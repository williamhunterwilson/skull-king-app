{
  "expo": {
    "name": "Skull King Scorer",
    "slug": "SkullKingApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/skull-king-icon.png",
    "resizeMode": "contain",
    "backgroundColor": "#1E1E1E",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/skull-king-icon.png",
        "backgroundColor": "#1E1E1E"
      },
      "package": "com.williamwilson.SkullKingApp"
    },
    "androidNavigationBar": {
      "backgroundColor": "#1E1E1E",
      "barStyle": "light-content"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1E1E1E"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d20cf5c9-e252-4f24-9cbf-5a6326156b54"
      }
    }
  }
}
